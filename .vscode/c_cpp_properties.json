{
  "configurations": [
    {
      "browse": {
        "databaseFilename": "${default}",
        "limitSymbolsToIncludedHeaders": true //智能提示只搜索包含的头文件，速度快
      },
      "includePath": [
        "${workspaceFolder}/src/**",
        "${workspaceFolder}/src/common/**",
        "/opt/ros/noetic/include/**",
        "/usr/local/include/**",
        "/usr/include/**"
      ],
      "name": "ROS",
      "intelliSenseMode": "gcc-x64",
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "gnu17",
      "cppStandard": "c++17",
      "configurationProvider": "ms-vscode.cmake-tools"
    }
  ],
  "version": 4
}
